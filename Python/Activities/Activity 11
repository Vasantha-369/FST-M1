fruit_shop = {
    "grapes": 10,
    "watermelon": 15,
    "orange": 8,
    "pear": 15,
    "banana" : 16
    
}

key_to_check = input("What are you looking for? ").lower()

if(key_to_check in fruit_shop):
    print("Yes, this fruit is available")
else:
    print("No, this fruit is not available")